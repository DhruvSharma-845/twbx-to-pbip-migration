          <parent-name>[leaf]</parent-name>
          <remote-name>Product Name</remote-name>
        </metadata-record>
        <metadata-record class='column'>
          <local-name>[Category]</local-name>
          <local-type>string</local-type>
          <parent-name>[leaf]</parent-name>
          <remote-name>Category</remote-name>
        </metadata-record>
        <metadata-record class='column'>
          <local-name>[Region]</local-name>
          <local-type>string</local-type>
          <parent-name>[leaf]</parent-name>
          <remote-name>Region</remote-name>
        </metadata-record>
        <metadata-record class='column'>
          <local-name>[Sales]</local-name>
          <local-type>real</local-type>
          <parent-name>[leaf]</parent-name>
          <remote-name>Sales</remote-name>
        </metadata-record>
        <metadata-record class='column'>
          <local-name>[Quantity]</local-name>
          <local-type>integer</local-type>
          <parent-name>[leaf]</parent-name>
          <remote-name>Quantity</remote-name>
        </metadata-record>
        <metadata-record class='column'>
          <local-name>[Profit]</local-name>
          <local-type>real</local-type>
          <parent-name>[leaf]</parent-name>
          <remote-name>Profit</remote-name>
        </metadata-record>
        <metadata-record class='column'>
          <local-name>[Customer_Name]</local-name>
          <local-type>string</local-type>
          <parent-name>[leaf]</parent-name>
          <remote-name>Customer Name</remote-name>
        </metadata-record>
      </metadata-records>
    </datasource>
  </datasources>
  <worksheets>
    <worksheet name='Sales by Category'>
      <table>
        <view>
          <datasources>
            <datasource caption='Sales Data' name='federated.sample_data' />
          </datasources>
          <datasource-dependencies datasource='federated.sample_data'>
            <column datatype='string' name='[Category]' role='dimension' type='nominal' />
            <column datatype='real' name='[Sales]' role='measure' type='quantitative' />
          </datasource-dependencies>
          <filter class='categorical' column='[federated.sample_data].[Category]'>
            <groupfilter function='level-members' level='[Category]' />
          </filter>
          <slices>
            <column>[federated.sample_data].[Category]</column>
          </slices>
          <aggregation value='true' />
        </view>
        <style>
          <style-rule element='cell'>
            <format attr='text-format' field='[federated.sample_data].[Sales]' value='c' />
          </style-rule>
        </style>
        <panes>
          <pane id='1' selection-relaxation-option='selection-relaxation-allow'>
            <view>
              <breakdown value='auto' />
            </view>
            <mark class='bar' />
            <encodings>
              <color column='[federated.sample_data].[Category]' />
            </encodings>
            <style>
              <style-rule element='mark'>
                <format attr='mark' value='bar' />
              </style-rule>
            </style>
          </pane>
        </panes>
        <rows>[federated.sample_data].[Sales]</rows>
        <cols>[federated.sample_data].[Category]</cols>
      </table>
    </worksheet>
    <worksheet name='Sales Trend'>
      <table>
        <view>
          <datasources>
            <datasource caption='Sales Data' name='federated.sample_data' />
          </datasources>
          <datasource-dependencies datasource='federated.sample_data'>
            <column datatype='date' name='[Order_Date]' role='dimension' type='ordinal' />
            <column datatype='real' name='[Sales]' role='measure' type='quantitative' />
          </datasource-dependencies>
          <aggregation value='true' />
        </view>
        <panes>
          <pane id='1' selection-relaxation-option='selection-relaxation-allow'>
            <view>
              <breakdown value='auto' />
            </view>
            <mark class='line' />
            <style>
              <style-rule element='mark'>
                <format attr='mark' value='line' />
              </style-rule>
            </style>
          </pane>
        </panes>
        <rows>SUM([federated.sample_data].[Sales])</rows>
        <cols>[federated.sample_data].[Order_Date]</cols>
      </table>
    </worksheet>
    <worksheet name='Regional Performance'>
      <table>
        <view>
          <datasources>
            <datasource caption='Sales Data' name='federated.sample_data' />
          </datasources>
          <datasource-dependencies datasource='federated.sample_data'>
            <column datatype='string' name='[Region]' role='dimension' type='nominal' />
            <column datatype='real' name='[Sales]' role='measure' type='quantitative' />
            <column datatype='real' name='[Profit]' role='measure' type='quantitative' />
          </datasource-dependencies>
          <aggregation value='true' />
        </view>
        <panes>
          <pane id='1' selection-relaxation-option='selection-relaxation-allow'>
            <view>
              <breakdown value='auto' />
            </view>
            <mark class='pie' />
            <style>
              <style-rule element='mark'>
                <format attr='mark' value='pie' />
              </style-rule>
            </style>
          </pane>
        </panes>
        <rows>[federated.sample_data].[Region]</rows>
        <cols>SUM([federated.sample_data].[Sales])</cols>
      </table>
    </worksheet>
    <worksheet name='Sales Details'>
      <table>
        <view>
          <datasources>
            <datasource caption='Sales Data' name='federated.sample_data' />
          </datasources>
          <datasource-dependencies datasource='federated.sample_data'>
            <column datatype='string' name='[Order_ID]' role='dimension' type='nominal' />
