  </preferences>
  <datasources>
    <datasource caption='Sales Data' inline='true' name='federated.sample_data'>
      <connection class='federated'>
        <named-connections>
          <named-connection caption='Sample Data' name='leaf'>
            <connection class='textscan' directory='/data' filename='sales.csv' />
          </named-connection>
        </named-connections>
        <cols>
          <map key='[Order_Date]' value='[leaf].[Order_Date]' />
          <map key='[Order_ID]' value='[leaf].[Order_ID]' />
          <map key='[Product_Name]' value='[leaf].[Product_Name]' />
          <map key='[Category]' value='[leaf].[Category]' />
          <map key='[Region]' value='[leaf].[Region]' />
          <map key='[Sales]' value='[leaf].[Sales]' />
          <map key='[Quantity]' value='[leaf].[Quantity]' />
          <map key='[Profit]' value='[leaf].[Profit]' />
          <map key='[Customer_Name]' value='[leaf].[Customer_Name]' />
        </cols>
      </connection>
      <aliases enabled='yes' />
      <column caption='Order Date' datatype='date' name='[Order_Date]' role='dimension' type='ordinal' />
      <column caption='Order ID' datatype='string' name='[Order_ID]' role='dimension' type='nominal' />
      <column caption='Product Name' datatype='string' name='[Product_Name]' role='dimension' type='nominal' />
      <column caption='Category' datatype='string' name='[Category]' role='dimension' type='nominal' />
      <column caption='Region' datatype='string' name='[Region]' role='dimension' type='nominal' />
      <column caption='Sales' datatype='real' name='[Sales]' role='measure' type='quantitative'>
        <calculation class='tableau' formula='' />
      </column>
      <column caption='Quantity' datatype='integer' name='[Quantity]' role='measure' type='quantitative' />
      <column caption='Profit' datatype='real' name='[Profit]' role='measure' type='quantitative' />
      <column caption='Customer Name' datatype='string' name='[Customer_Name]' role='dimension' type='nominal' />
      <column caption='Total Sales' datatype='real' name='[Calculation_TotalSales]' role='measure' type='quantitative'>
        <calculation class='tableau' formula='SUM([Sales])' />
      </column>
      <column caption='Total Profit' datatype='real' name='[Calculation_TotalProfit]' role='measure' type='quantitative'>
        <calculation class='tableau' formula='SUM([Profit])' />
      </column>
      <column caption='Profit Ratio' datatype='real' name='[Calculation_ProfitRatio]' role='measure' type='quantitative'>
        <calculation class='tableau' formula='SUM([Profit]) / SUM([Sales])' />
      </column>
      <column caption='Average Order Value' datatype='real' name='[Calculation_AvgOrderValue]' role='measure' type='quantitative'>
        <calculation class='tableau' formula='AVG([Sales])' />
      </column>
      <column caption='Order Count' datatype='integer' name='[Calculation_OrderCount]' role='measure' type='quantitative'>
        <calculation class='tableau' formula='COUNTD([Order_ID])' />
      </column>
      <column caption='Running Total Sales' datatype='real' name='[Calculation_RunningTotal]' role='measure' type='quantitative'>
        <calculation class='tableau' formula='RUNNING_SUM(SUM([Sales]))' />
      </column>
      <column caption='Regional Sales Fixed' datatype='real' name='[Calculation_LOD]' role='measure' type='quantitative'>
        <calculation class='tableau' formula='{FIXED [Region] : SUM([Sales])}' />
      </column>
      <extract enabled='false' />
      <metadata-records>
        <metadata-record class='column'>
          <local-name>[Order_Date]</local-name>
          <local-type>date</local-type>
          <parent-name>[leaf]</parent-name>
          <remote-name>Order Date</remote-name>
        </metadata-record>
        <metadata-record class='column'>
          <local-name>[Order_ID]</local-name>
          <local-type>string</local-type>
          <parent-name>[leaf]</parent-name>
          <remote-name>Order ID</remote-name>
